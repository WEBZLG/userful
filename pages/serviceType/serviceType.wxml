<!--pages/serviceType/serviceType.wxml-->
<view>
	<view class="flex">
		<van-sidebar active-key="{{ activeKey }}" bind:change="onChange">
			<van-sidebar-item wx:for="{{iconNav}}" wx:key="index" title="{{item.title}}" />
		</van-sidebar>
		<view class="right">
      <view class="banner">
        <van-image width="100%" height="100" src="https://img.yzcdn.cn/vant/cat.jpeg" />
      </view>
			<view class="type-list">
				<view class="caption">公司注册</view>
				<view class="child-list">
					<van-grid column-num="3" gutter="{{ 10 }}">
						<van-grid-item use-slot wx:for="{{ 4 }}" wx:key="index" catchtap="onDetail" >
                有限公司注册
            </van-grid-item>
					</van-grid>
				</view>
			</view>
		</view>
	</view>

</view>