<!--miniprogram/custom-tab-bar/index.wxml-->
<van-tabbar active="{{ selected }}" active-color="#1296db" inactive-color="#333333">
	<van-tabbar-item  wx:for="{{list}}" wx:key="index" data-path="{{item.pagePath}}" data-index="{{index}}" bindtap="switchTab" info="3">
		<image slot="icon" src="{{ item.iconPath }}" mode="aspectFit" style="width: 30px; height: 18px;" />
		<image slot="icon-active" src="{{ item.selectedIconPath }}" mode="aspectFit" style="width: 30px; height: 18px;" />
		{{item.text}}
	</van-tabbar-item>
</van-tabbar>
